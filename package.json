{
	"name": "cli",
	"version": "0.0.0",
	"author": "Choi Yoons @Nipol",
	"bin": {
		"cli": "./bin/run",
		"lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
	},
	"bugs": "https://github.com/Nipol/cli/issues",
	"dependencies": {
		"@oclif/command": "^1.5.13",
		"@oclif/config": "^1.12.12",
		"@oclif/plugin-help": "^2.1.6",
		"class-transformer": "^0.2.0",
		"form-data": "^2.3.3",
		"nock": "^10.0.6",
		"node-fetch": "^2.5.0",
		"reflect-metadata": "^0.1.13",
		"tslib": "^1.9.3",
		"wretch": "^1.5.2"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^0.1.3",
		"@oclif/dev-cli": "^1.22.0",
		"@oclif/test": "^1.2.4",
		"@types/chai": "^4.1.7",
		"@types/faker": "^4.1.4",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.5",
		"@typescript-eslint/eslint-plugin": "^1.7.0",
		"@typescript-eslint/parser": "^1.7.0",
		"chai": "^4.2.0",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^4.2.0",
		"eslint-plugin-prettier": "^3.0.1",
		"faker": "^4.1.0",
		"globby": "^8.0.2",
		"mocha": "^5.2.0",
		"mockttp": "^0.14.3",
		"nyc": "^14.1.0",
		"prettier": "^1.17.0",
		"source-map-support": "^0.5.12",
		"ts-node": "^8.1.0",
		"typescript": "^3.4.3"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/Nipol/cli",
	"keywords": [
		"oclif"
	],
	"license": "LGPL3",
	"main": "lib/index.js",
	"oclif": {
		"commands": "./lib/commands",
		"bin": "cli",
		"plugins": [
			"@oclif/plugin-help"
		],
		"hooks": {
			"init": "./lib/hooks/init/wretch"
		},
		"topics": {
			"integration": {
				"description": "manage integrations"
			}
		}
	},
	"repository": "Nipol/cli",
	"scripts": {
		"postpack": "rm -f oclif.manifest.json",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"test": "tsc && nyc mocha --forbid-only \"src/**/*.spec.ts\"",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts"
}
