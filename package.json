			"@oclif/plugin-autocomplete"
		],
		"hooks": {
			"init": "./lib/hooks/init/wretch"
		},
		"topics": {
			"integration": {
				"description": "manage integrations"
			}
		}
	},
	"repository": "HAECHI-LABS/henesis-cli",
	"scripts": {
		"tsc": "tsc -b",
		"postpack": "rm -f oclif.manifest.json",
		"prepack": "tsc -b && oclif-dev manifest && oclif-dev readme",
